{
  "manifest_version": 2,
  "name": "Content Blocker",
  "version": "0.0.1",
  "background": {
    "run_at": "document_start",
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
