{
  "manifest_version": 2,
  "name": "Pokemon Go Away!",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/jquery.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "assets/icon.png"
  },
  "permissions": [
    "storage"
  ]
}
