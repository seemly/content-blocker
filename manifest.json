{
  "manifest_version": 2,
  "name": "Pokemon Go Away!",
  "version": "0.0.1",
  "background": {
    "run_at": "document_start",
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/jquery.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage"],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
